<h2>latest dishes</h2>

<div class="all-dishes">
    <% for (let game of games) {%>
        <div class="ui-card">
            <img src="<%= game.image_url %>" alt="">
            <h2>
                <a href="/dishes/<%= game.id %>"><%= game.title %></a>
            </h2>
            <h3>
                Number of users: <%= game.player_count %>
            </h3>
        </div>

        <% } %>
</div>