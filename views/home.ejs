<h2>latest dishes</h2>

<div class="all-dishes">
    <% for (let dish of dishes) {%>
        <div class="ui-card">
            <img src="<%= dish.image_url %>" alt="">
            <h2>
                <a href="/dishes/<%= dish.id %>"><%= dish.title %></a>
            </h2>
            <h3>
                shared by: <%= dish.email %>
            </h3>
        </div>

        <% } %>
</div>